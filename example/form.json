{
  "name": "xuqserve-combine",
  "framwork": "vue",
  "uiComponent": "ant-design-vue",
  "template": "table",
  "action": [
    "add-modal"
  ],
  "tableColums": [
    {
      "label": "ID",
      "field": "id",
      "custom": "text"
    },
    {
      "label": "产品名称",
      "field": "product_name"
    },
    {
      "label": "价格（元）",
      "field": "price"
    },
    {
      "label": "可优惠",
      "field": "is_preferential",
