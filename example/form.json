    },
    {
      "label": "可退费",
      "field": "is_refundable",
      "custom": true
    },
    {
      "label": "收费主体",
      "field": "collection_name"
    },
    {
      "label": "所属机构",
      "field": "organization_name"
    },
    {
      "label": "费用类型",
      "field": "expense_type_name"
    }
  ],
  "form": [
    {
      "type": "select",
      "field": "semester_id",
      "label": "关联学期"
